alias hero-core='export ROS_MASTER_URI=http://hero1.local:11311'

alias sshhero1='until ssh -qo ConnectTimeout=1 administrator@hero1.local; do echo waiting for hero1 to come online...; sleep 2; done'
alias sshhero2='until ssh -qo ConnectTimeout=1 amigo@hero2.local; do echo waiting for hero2 to come online...; sleep 2; done'
